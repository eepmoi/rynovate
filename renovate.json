{
  "extends": [
    ":prConcurrentLimit10",
    ":prHourlyLimitNone",
    ":prNotPending",
    ":rebaseStalePrs",
    ":renovatePrefix",
    ":semanticCommitsDisabled",
    ":timezone(Australia/Melbourne)",
    ":unpublishSafe",
    "preview:buildkite",
    "preview:dockerCompose"
  ],
  "packageRules": [
    {
      "commitMessageExtra": "{{newValue}}",
      "commitMessageTopic": "{{depName}}",
      "managers": ["buildkite", "gomod", "npm"]
    },
    {
      "commitMessageExtra": "",
      "groupName": "npm definitelyTyped",
      "managers": ["npm"],
      "packagePatterns": ["^@types/"]
    },
    {
      "commitMessageExtra": "",
      "depTypeList": ["devDependencies"],
      "excludePackageNames": ["braid-design-system", "sku"],
      "excludePackagePatterns": ["^@?seek", "seek$", "^@types/"],
      "groupName": "npm devDependencies",
      "managers": ["npm"]
    },
    {
      "commitMessageExtra": "",
      "depTypeList": ["peerDependencies"],
      "excludePackageNames": ["braid-design-system", "sku"],
      "excludePackagePatterns": ["^@?seek", "seek$", "^@types/"],
      "groupName": "npm peerDependencies",
      "managers": ["npm"]
    },
    {
      "commitMessageExtra": "",
      "excludePackagePatterns": ["^seek-oss/"],
      "groupName": "Buildkite plugins",
      "managers": ["buildkite"],
      "schedule": "before 7am on Friday"
    },
    {
      "commitMessageExtra": "",
      "group": {
        "commitMessageTopic": "{{groupName}}"
      },
      "groupName": "Docker images",
      "managers": ["docker-compose", "dockerfile"],
      "schedule": "before 7am on Friday"
    },
    {
      "packageNames": ["braid-design-system", "sku"],
      "packagePatterns": ["^@?seek", "seek$"],
      "schedule": "before 7am on every weekday"
    },
    {
      "depTypeList": ["dependencies", "devDependencies"],
      "rangeStrategy": "replace"
    },
    {
      "depTypeList": ["peerDependencies"],
      "rangeStrategy": "widen"
    }
  ],
  "commitMessageAction": "",
  "commitMessagePrefix": "deps:",
  "ignoreNpmrcFile": true,
  "lazyGrouping": false,
  "postUpdateOptions": ["npmDedupe", "yarnDedupeHighest"],
  "prNotPendingHours": 1,
  "schedule": "before 7am on Monday"
}
